{#
/**
 * @file
 * Default theme implementation to display a schedule slot.
 *
 * Available variables:
 * - schedule_slot: The schedule slot entity.
 * - content: All schedule slot items. Use {{ content }} to print them all,
 *   or print a subset such as {{ content.title }}.
 *
 * @ingroup themeable
 */
#}
<article{{ attributes.addClass('schedule-slot') }}>
  {% if content %}
    {{ content }}
  {% endif %}
</article>
